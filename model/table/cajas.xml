<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : cajas.xml
    Description:
        Estructura de la tabla cajas.
-->
<tabla>
   <columna>
      <nombre>id</nombre>
      <tipo>serial</tipo>
      <nulo>NO</nulo>
      <defecto>nextval('cajas_id_seq'::regclass)</defecto>
   </columna>
   <columna>
      <nombre>fs_id</nombre>
      <tipo>integer</tipo>
      <nulo>NO</nulo>
   </columna>
   <columna>
      <nombre>codagente</nombre>
      <tipo>character varying(10)</tipo>
      <nulo>NO</nulo>
   </columna>
   <columna>
      <nombre>f_inicio</nombre>
      <tipo>timestamp without time zone</tipo>
      <nulo>NO</nulo>
   </columna>
   <columna>
      <nombre>d_inicio</nombre>
      <tipo>double precision</tipo>
      <nulo>NO</nulo>
      <defecto>0</defecto>
   </columna>
   <columna>
      <nombre>f_fin</nombre>
      <tipo>timestamp without time zone</tipo>
      <nulo>YES</nulo>
   </columna>
   <columna>
      <nombre>d_fin</nombre>
      <tipo>double precision</tipo>
      <nulo>YES</nulo>
   </columna>
   <columna>
      <nombre>tickets</nombre>
      <tipo>integer</tipo>
      <default>0</default>
      <nulo>YES</nulo>
   </columna>
   <columna>
      <nombre>ip</nombre>
      <tipo>character varying(40)</tipo>
      <nulo>YES</nulo>
   </columna>
   <restriccion>
      <nombre>cajas_pkey</nombre>
      <consulta>PRIMARY KEY (id)</consulta>
   </restriccion>
   <restriccion>
      <nombre>ca_cajas_agentes2</nombre>
      <consulta>FOREIGN KEY (codagente)
      REFERENCES agentes (codagente)
      ON DELETE RESTRICT ON UPDATE CASCADE</consulta>
   </restriccion>
</tabla>