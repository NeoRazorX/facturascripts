<view>
    <columns>
        <column name="id" order="100">
            <widget type="integer" fieldname="id" />
        </column>
        <column name="idapikey" order="110">
            <widget type="text" fieldname="idapikey" onclick="AddApiKey" />
        </column>
        <column name="resource" order="120">
            <widget type="text" fieldname="apikey" />
        </column>
        <column name="allow_delete" display="right" order="130">
            <widget type="text" fieldname="allow_delete" />
        </column>
        <column name="allow_get" display="right" order="130">
            <widget type="text" fieldname="allow_get" />
        </column>
        <column name="allow_post" display="right" order="130">
            <widget type="text" fieldname="allow_post" />
        </column>
        <column name="allow_put" display="right" order="130">
            <widget type="text" fieldname="allow_put" />
        </column>
    </columns>
    <filters>
    </filters>
</view>
