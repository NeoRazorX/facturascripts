<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : clientes.xml
    Author     : Carlos Garcia Gomez
    Description:
        Structure for the clientes table.
-->
<table>
<column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('emailsent_id_seq'::regclass)</default>
    </column>
    <column>
        <name>fecha</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>user</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>subject</name>
        <type>character varying(300)</type>
    </column>
    <column>
        <name>text</name>
        <type>text</type>
    </column>
    <column>
        <name>addressee</name>
        <type>character varying(100)</type>
    </column>
    <constraint>
        <name>id_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
</table>
