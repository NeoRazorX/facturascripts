<?xml version="1.0" encoding="UTF-8"?>
<table>
    <column>
        <name>alias</name>
        <type>text</type>
    </column>
    <column>
        <name>creation_date</name>
        <type>timestamp</type>
    </column>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
