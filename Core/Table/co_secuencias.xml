<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : co_secuencias.xml
    Author     : Carlos Garcia Gomez
    Description:
        Estructura de la tabla co_secuencias.
-->
<table>
    <column>
        <name>codejercicio</name>
        <type>character varying(4)</type>
        <null>NO</null>
    </column>
    <column>
        <name>descripcion</name>
        <type>character varying(100)</type>
        <null>YES</null>
    </column>
    <column>
        <name>idsecuencia</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('co_secuencias_idsecuencia_seq'::regclass)</default>
    </column>
    <column>
        <name>nombre</name>
        <type>character varying(50)</type>
        <null>NO</null>
    </column>
    <column>
        <name>valor</name>
        <type>integer</type>
        <null>YES</null>
    </column>
    <column>
        <name>valorout</name>
        <type>integer</type>
        <null>YES</null>
    </column>
    <constraint>
        <name>co_secuencias_pkey</name>
        <type>PRIMARY KEY (idsecuencia)</type>
    </constraint>
    <constraint>
        <name>ca_co_secuencias_ejercicios</name>
        <type>FOREIGN KEY (codejercicio)
            REFERENCES ejercicios (codejercicio)
            ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
</table>