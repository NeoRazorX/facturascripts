<table>
    <column>
        <name>component</name>
        <type>character varying(50)</type>
        <null>NO</null>
    </column>
    <column>
        <name>version</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>location</name>
        <type>integer</type>
        <null>NO</null>
        <default>99</default>
    </column>
    <constraint>
        <name>dashboard_pkey</name>
        <type>PRIMARY KEY (component)</type>
    </constraint>
</table>