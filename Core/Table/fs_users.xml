<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : fs_users.xml
    Author     : Carlos Garcia Gomez
    Description:
        Estructura de la tabla fs_users.
-->
<table>
    <column>
        <name>nick</name>
        <type>character varying(50)</type>
        <null>NO</null>
    </column>
    <column>
        <name>password</name>
        <type>character varying(255)</type>
        <null>NO</null>
    </column>
    <column>
        <name>email</name>
        <type>character varying(100)</type>
        <null>YES</null>
    </column>
    <column>
        <name>logkey</name>
        <type>character varying(100)</type>
        <null>YES</null>
    </column>
    <column>
        <name>lastactivity</name>
        <type>timestamp</type>
        <null>YES</null>
    </column>
    <column>
        <name>lastip</name>
        <type>character varying(40)</type>
        <null>YES</null>
    </column>
    <column>
        <name>admin</name>
        <type>boolean</type>
        <null>NO</null>
        <default>false</default>
    </column>
    <column>
        <name>enabled</name>
        <type>boolean</type>
        <null>NO</null>
        <default>true</default>
    </column>
    <column>
        <name>langcode</name>
        <type>character varying(10)</type>
        <null>YES</null>
    </column>
    <column>
        <name>homepage</name>
        <type>character varying(30)</type>
        <null>YES</null>
    </column>
    <column>
        <name>idempresa</name>
        <type>integer</type>
        <null>YES</null>
    </column>
    <constraint>
        <name>fs_users_pkey</name>
        <type>PRIMARY KEY (nick)</type>
    </constraint>
    <constraint>
        <name>ca_fs_users_pages</name>
        <type>FOREIGN KEY (homepage) REFERENCES fs_pages (name) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_fs_users_company</name>
        <type>FOREIGN KEY (idempresa) REFERENCES empresas (id) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
</table>