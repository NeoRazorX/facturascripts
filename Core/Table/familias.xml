<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : familias.xml
    Author     : Carlos Garcia Gomez
    Description:
        Structure for the familias table.
-->
<table>
    <column>
        <name>codfamilia</name>
        <type>character varying(8)</type>
        <null>NO</null>
    </column>
    <column>
        <name>codsubcuentacom</name>
        <type>character varying(15)</type>
    </column>
    <column>
        <name>codsubcuentairpfcom</name>
        <type>character varying(15)</type>
    </column>
    <column>
        <name>codsubcuentaven</name>
        <type>character varying(15)</type>
    </column>
    <column>
        <name>descripcion</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    <column>
        <name>madre</name>
        <type>character varying(8)</type>
    </column>
    <constraint>
        <name>familias_pkey</name>
        <type>PRIMARY KEY (codfamilia)</type>
    </constraint>
    <constraint>
        <name>ca_familia_madre</name>
        <type>FOREIGN KEY (madre) REFERENCES familias (codfamilia) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
</table>