<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : agentes.xml
    Author     : Carlos Garcia Gomez
    Description:
        Structure for the agentes table.
-->
<table>
    <column>
        <name>banco</name>
        <type>character varying(34)</type>
    </column>
    <column>
        <name>cargo</name>
        <type>character varying(100)</type>
    </column>
    <column>
        <name>cifnif</name>
        <type>character varying(30)</type>
    </column>
    <column>
        <name>ciudad</name>
        <type>character varying(100)</type>
    </column>
    <column>
        <name>codagente</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>codpostal</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>debaja</name>
        <type>boolean</type>
        <default>false</default>
    </column>
    <column>
        <name>direccion</name>
        <type>character varying(100)</type>
    </column>
    <column>
        <name>email</name>
        <type>character varying(100)</type>
    </column>
    <column>
        <name>fechaalta</name>
        <type>date</type>
    </column>
    <column>
        <name>fechabaja</name>
        <type>date</type>
    </column>
    <column>
        <name>fechanacimiento</name>
        <type>date</type>
    </column>
    <column>
        <name>nombre</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    <column>
        <name>observaciones</name>
        <type>text</type>
    </column>
    <column>
        <name>porcomision</name>
        <type>double precision</type>
    </column>
    <column>
        <name>provincia</name>
        <type>character varying(100)</type>
    </column>
    <column>
        <name>seg_social</name>
        <type>character varying(100)</type>
    </column>
    <column>
        <name>telefono1</name>
        <type>character varying(30)</type>
    </column>
    <column>
        <name>telefono2</name>
        <type>character varying(30)</type>
    </column>
    <column>
        <name>codretencion</name>
        <type>character varying(10)</type>
        <default>NULL</default>
    </column>
    <column>
        <name>irpf</name>
        <type>double precision</type>
        <default>0</default>
    </column>
    <constraint>
        <name>agentes_pkey</name>
        <type>PRIMARY KEY (codagente)</type>
    </constraint>
    <constraint>
        <name>ca_agentes_retenciones</name>
        <type>FOREIGN KEY (codretencion) REFERENCES retenciones (codretencion) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>

</table>