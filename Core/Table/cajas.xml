<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : cajas.xml
    Author     : Carlos Garcia Gomez
    Description:
        Estructura de la tabla cajas.
-->
<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('cajas_id_seq'::regclass)</default>
    </column>
    <column>
        <name>fs_id</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>codagente</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>f_inicio</name>
        <type>timestamp without time zone</type>
        <null>NO</null>
        <default>CURRENT_TIMESTAMP</default>
    </column>
    <column>
        <name>d_inicio</name>
        <type>double precision</type>
        <null>NO</null>
        <default>0</default>
    </column>
    <column>
        <name>f_fin</name>
        <type>timestamp without time zone</type>
        <null>YES</null>
    </column>
    <column>
        <name>d_fin</name>
        <type>double precision</type>
        <null>YES</null>
    </column>
    <column>
        <name>tickets</name>
        <type>integer</type>
        <default>0</default>
        <null>YES</null>
    </column>
    <column>
        <name>ip</name>
        <type>character varying(40)</type>
        <null>YES</null>
    </column>
    <constraint>
        <name>cajas_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
</table>