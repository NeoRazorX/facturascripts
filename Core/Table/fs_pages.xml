<table>
    <column>
        <name>name</name>
        <type>character varying(30)</type>
        <null>NO</null>
    </column>
    <column>
        <name>title</name>
        <type>character varying(50)</type>
        <null>NO</null>
    </column>
    <column>
        <name>menu</name>
        <type>character varying(20)</type>
        <null>YES</null>
    </column>
    <column>
        <name>submenu</name>
        <type>character varying(20)</type>
        <null>YES</null>
    </column>
    <column>
        <name>showonmenu</name>
        <type>boolean</type>
        <null>NO</null>
        <default>true</default>
    </column>
    <column>
        <name>orden</name>
        <type>integer</type>
        <null>NO</null>
        <default>100</default>
    </column>
    <column>
        <name>icon</name>
        <type>character varying(50)</type>
        <null>YES</null>
    </column>
    <constraint>
        <name>fs_pages_pkey</name>
        <type>PRIMARY KEY (name)</type>
    </constraint>
</table>