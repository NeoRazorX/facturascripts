<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : fs_api_keys.xml
    Author     : Joe Nilson Zegarra Galvez
    Description:
        Estructura de la tabla fs_api_keys.
-->
<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('fs_api_keys_id_seq'::regclass)</default>
    </column>
    <column>
        <name>apikey</name>
        <type>character varying(250)</type>
        <null>NO</null>
    </column>
    <column>
        <name>descripcion</name>
        <type>character varying(150)</type>
        <null>NO</null>
    </column>
    <column>
        <name>enabled</name>
        <type>boolean</type>
        <null>NO</null>
        <default>false</default>
    </column>
    <column>
        <name>f_alta</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>f_baja</name>
        <type>date</type>
        <null>YES</null>
    </column>
    <column>
        <name>nick</name>
        <type>character varying(50)</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>fs_api_keys_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
</table>