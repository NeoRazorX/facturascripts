        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>numero</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>tipodocumento</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>YES</nulo>
    </columna>
    <restriccion>
        <nombre>ca_co_asientos_ejercicios2</nombre>
        <consulta>FOREIGN KEY (codejercicio)
            REFERENCES ejercicios (codejercicio)
            ON DELETE RESTRICT ON UPDATE CASCADE</consulta>
    </restriccion>
    <restriccion>
        <nombre>co_asientos_pkey</nombre>
        <consulta>PRIMARY KEY (idasiento)</consulta>
    </restriccion>
</tabla>