<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : co_asientos.xml
    Author     : Carlos Garcia Gomez
    Description:
        Structure for the co_asientos table.
-->
<table>
    <column>
        <name>codejercicio</name>
        <type>character varying(4)</type>
        <null>NO</null>
    </column>
    <column>
        <name>codplanasiento</name>
        <type>character varying(20)</type>
        <null>YES</null>
    </column>
    <column>
        <name>concepto</name>
        <type>character varying(255)</type>
        <null>YES</null>
    </column>
    <column>
        <name>documento</name>
        <type>character varying(30)</type>
        <null>YES</null>
    </column>
    <column>
        <name>editable</name>
        <type>boolean</type>
        <null>NO</null>
    </column>
    <column>
        <name>fecha</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>idasiento</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('co_asientos_idasiento_seq'::regclass)</default>
    </column>
    <column>
        <name>idconcepto</name>
        <type>character varying(4)</type>
        <null>YES</null>
    </column>
    <column>
        <name>idempresa</name>
        <type>integer</type>
        <null>YES</null>
    </column>
    <column>
        <name>importe</name>
        <type>double precision</type>
        <null>YES</null>
    </column>
    <column>
        <name>numero</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>tipodocumento</name>
        <type>character varying(25)</type>
        <null>YES</null>
    </column>
    <constraint>
        <name>ca_co_asientos_ejercicios2</name>
        <type>FOREIGN KEY (codejercicio)
            REFERENCES ejercicios (codejercicio)
            ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>co_asientos_pkey</name>
        <type>PRIMARY KEY (idasiento)</type>
    </constraint>
</table>