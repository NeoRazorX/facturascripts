        <nombre>codejercicio</nombre>
        <tipo>character varying(4)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>codplanasiento</nombre>
        <tipo>character varying(20)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>concepto</nombre>
        <tipo>character varying(255)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>documento</nombre>
        <tipo>character varying(30)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>editable</nombre>
        <tipo>boolean</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fecha</nombre>
        <tipo>date</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>idasiento</nombre>
        <tipo>serial</tipo>
