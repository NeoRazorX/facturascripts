<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : transstock.xml
    Author     : Carlos Garcia Gomez
    Description:
        Structure for the transstock table.
-->
<table>
    <column>
        <name>idtrans</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('transstock_idtrans_seq'::regclass)</default>
    </column>
    <column>
        <name>fecha</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>hora</name>
        <type>time without time zone</type>
        <null>NO</null>
    </column>
    <column>
        <name>sourcecodalmacen</name>
        <type>character varying(4)</type>
        <comment>Warehouse from which the goods leave</comment>
    </column>
    <column>
        <name>destinationcodalmacen</name>
        <type>character varying(4)</type>
        <comment>Warehouse where the goods arrives</comment>
    </column>
    <column>
        <name>nick</name>
        <type>character varying(12)</type>
    </column>
    <constraint>
        <name>transstock_pkey</name>
        <type>PRIMARY KEY (idtrans)</type>
    </constraint>
</table>
