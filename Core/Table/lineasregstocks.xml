<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : lineasregstocks.xml
    Author     : Carlos Garcia Gomez
    Description:
        Structure for the lineasregstocks table.
-->
<table>
    <column>
        <name>cantidadfin</name>
        <type>double precision</type>
        <null>NO</null>
        <default>0</default>
    </column>
    <column>
        <name>cantidadini</name>
        <type>double precision</type>
        <null>NO</null>
        <default>0</default>
    </column>
    <column>
        <name>codalmacendest</name>
        <type>character varying(4)</type>
    </column>
    <column>
        <name>fecha</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>hora</name>
        <type>time without time zone</type>
        <null>NO</null>
    </column>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('lineasregstocks_id_seq'::regclass)</default>
    </column>
    <column>
        <name>idstock</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>motivo</name>
        <type>text</type>
    </column>
    <column>
        <name>nick</name>
        <type>character varying(12)</type>
    </column>
    <constraint>
        <name>lineasregstocks_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>ca_lineasregstocks_stocks</name>
        <type>FOREIGN KEY (idstock) REFERENCES stocks (idstock) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
</table>
