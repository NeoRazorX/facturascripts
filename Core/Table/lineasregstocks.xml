<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : lineasregstocks.xml
    Author     : Carlos Garcia Gomez
    Description:
        Structure for the lineasregstocks table.
-->
<table>
    <column>
        <name>idreg</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('lineasregstocks_idreg_seq'::regclass)</default>
    </column>
    <column>
        <name>fecha</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>hora</name>
        <type>time without time zone</type>
        <null>NO</null>
    </column>
    <column>
        <name>codalmacen</name>
        <type>character varying(4)</type>
    </column>
    <column>
        <name>referencia</name>
        <type>character varying(30)</type>
        <null>NO</null>
        <comment>Reference of the main product</comment>
    </column>
    <column>
        <name>codcombinacion</name>
        <type>character varying(18)</type>
        <default>null</default>
        <comment>Reference of the combination, if exists. Null if not exist.</comment>
    </column>
    <column>
        <name>cantidadini</name>
        <type>double precision</type>
        <null>NO</null>
        <default>0</default>
    </column>
    <column>
        <name>cantidadfin</name>
        <type>double precision</type>
        <null>NO</null>
        <default>0</default>
    </column>
    <column>
        <name>motivo</name>
        <type>text</type>
    </column>
    <column>
        <name>nick</name>
        <type>character varying(12)</type>
    </column>
    <constraint>
        <name>lineasregstocks_pkey</name>
        <type>PRIMARY KEY (idreg)</type>
    </constraint>
    <constraint>
        <name>ca_lineasregstocks_almacenes</name>
        <type>FOREIGN KEY (codalmacen) REFERENCES almacenes (codalmacen) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_lineasregstocks_stocks</name>
        <type>FOREIGN KEY (referencia) REFERENCES stocks (referencia) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
</table>
