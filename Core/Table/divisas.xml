<table>
    <column>
        <name>coddivisa</name>
        <type>character varying(3)</type>
        <null>NO</null>
    </column>
    <column>
        <name>codiso</name>
        <type>character varying(5)</type>
    </column>
    <column>
        <name>descripcion</name>
        <type>character varying(100)</type>
    </column>
    <column>
        <name>simbolo</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>tasaconv</name>
        <type>double precision</type>
        <null>NO</null>
    </column>
    <column>
        <name>tasaconvcompra</name>
        <type>double precision</type>
    </column>
    <constraint>
        <name>divisas_pkey</name>
        <type>PRIMARY KEY (coddivisa)</type>
    </constraint>
</table>