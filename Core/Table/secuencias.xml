<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : secuencias.xml
    Author     : Carlos Garcia Gomez
    Description:
        Estructura de la tabla secuencias.
-->
<tabla>
    <columna>
        <nombre>descripcion</nombre>
        <tipo>character varying(100)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>id</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>idsec</nombre>
        <tipo>serial</tipo>
        <nulo>NO</nulo>
        <defecto>nextval('secuencias_idsec_seq'::regclass)</defecto>
    </columna>
    <columna>
        <nombre>nombre</nombre>
        <tipo>character varying(50)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>valor</nombre>
        <tipo>integer</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>valorout</nombre>
        <tipo>integer</tipo>
        <nulo>YES</nulo>
    </columna>
    <restriccion>
        <nombre>secuencias_pkey</nombre>
        <consulta>PRIMARY KEY (idsec)</consulta>
    </restriccion>
    <restriccion>
        <nombre>ca_secuencias_secuenciasejercicios</nombre>
        <consulta>FOREIGN KEY (id)
            REFERENCES secuenciasejercicios (id)
            ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
</tabla>