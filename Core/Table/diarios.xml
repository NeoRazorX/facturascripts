<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : diarios.xml
    Author     : Raul Jimenez
    Description:
        Structure for the diarios table.
-->
<table>
    <column>
        <name>coddiario</name>
        <type>character varying(4)</type>
        <null>NO</null>
    </column>
    <column>
        <name>nombre</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    
    <constraint>
        <name>diarios_pkey</name>
        <type>PRIMARY KEY (coddiario)</type>
    </constraint>
</table>