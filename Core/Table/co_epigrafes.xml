<tabla>
    <columna>
        <nombre>codejercicio</nombre>
        <tipo>character varying(4)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>codepigrafe</nombre>
        <tipo>character varying(10)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>descripcion</nombre>
        <tipo>character varying(255)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>idepigrafe</nombre>
        <tipo>serial</tipo>
        <nulo>NO</nulo>
