<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : fabricantes.xml
    Author     : Carlos Garcia Gomez
    Description:
       Structure for the fabricantes table.
-->
<table>
    <column>
        <name>codfabricante</name>
        <type>character varying(8)</type>
        <null>NO</null>
    </column>
    <column>
        <name>nombre</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    <column>
        <name>numproductos</name>
        <type>integer</type>
        <null>NO</null>
        <default>0</default>
    </column>
    <constraint>
        <name>fabricantes_pkey</name>
        <type>PRIMARY KEY (codfabricante)</type>
    </constraint>
</table>