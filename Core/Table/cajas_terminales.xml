<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : cajas.xml
    Author     : Carlos Garcia Gomez
    Description:
        Estructura de la tabla cajas_terminales.
-->
<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('cajas_terminales_id_seq'::regclass)</default>
    </column>
    <column>
        <name>codalmacen</name>
        <type>character varying(4)</type>
        <null>YES</null>
    </column>
    <column>
        <name>codserie</name>
        <type>character varying(4)</type>
        <null>NO</null>
    </column>
    <column>
        <name>codcliente</name>
        <type>character varying(10)</type>
        <null>YES</null>
    </column>
    <column>
        <name>tickets</name>
        <type>text</type>
        <null>YES</null>
    </column>
    <column>
        <name>anchopapel</name>
        <type>integer</type>
    </column>
    <column>
        <name>comandocorte</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>comandoapertura</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>num_tickets</name>
        <type>integer</type>
    </column>
    <column>
        <name>sin_comandos</name>
        <type>boolean</type>
        <default>false</default>
    </column>
    <constraint>
        <name>cajas_terminales_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
</table>