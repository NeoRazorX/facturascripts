    Description:
        Estructura de la tabla fs_pages_options.
-->
<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('fs_pages_options_id_seq'::regclass)</default>
    </column>
    <column>
        <name>name</name>
        <type>character varying(30)</type>
        <null>NO</null>
    </column>
    <column>
        <name>nick</name>
        <type>character varying(50)</type>
        <null>YES</null>
    </column>
    <column>
        <name>rows</name>
        <type>text</type>
        <null>YES</null>
    </column>
    <column>
        <name>columns</name>
        <type>text</type>
        <null>YES</null>
    </column>
    <column>
        <name>filters</name>
        <type>text</type>
        <null>YES</null>
    </column>
    <constraint>
        <name>fs_pages_options_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>uniq_pages_options</name>
        <type>UNIQUE (name, nick)</type>
    </constraint>
    <constraint>
        <name>ca_fs_pagesoptions_users</name>
        <type>FOREIGN KEY (nick) REFERENCES fs_users (nick) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
</table>