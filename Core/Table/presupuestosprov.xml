    <column>
        <name>codigo</name>
        <type>character varying(20)</type>
        <null>NO</null>
    </column>
    <column>
        <name>coddivisa</name>
        <type>character varying(3)</type>
        <null>NO</null>
    </column>
    <column>
        <name>codpago</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
