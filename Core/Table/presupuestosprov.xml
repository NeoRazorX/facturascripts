        <type>date</type>
    </column>
    <column>
        <name>hora</name>
        <type>time without time zone</type>
    </column>
    <column>
        <name>idempresa</name>
        <type>integer</type>
    </column>
    <column>
        <name>idestado</name>
        <type>integer</type>
    </column>
    <column>
        <name>idpedido</name>
        <type>integer</type>
    </column>
    <column>
        <name>idpresupuesto</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('presupuestosprov_idpresupuesto_seq'::regclass)</default>
    </column>
    <column>
        <name>irpf</name>
        <type>double precision</type>
    </column>
    <column>
        <name>neto</name>
        <type>double precision</type>
        <null>NO</null>
    </column>
    <column>
        <name>nombre</name>
        <type>character varying(100)</type>
